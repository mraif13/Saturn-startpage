{
  "manifest_version": 2,
  "name": "Saturn Startpage",
  "short_name": "saturnstartpage",
  "version": "7.2.1",
  "description": "Saturn Startpage: Free, open-source new tab replacement designed for speed and UX.",
  "author": "raifcoonjah",

  "icons": {
    "64": "/assets/img/icon64.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "chrome_settings_overrides": {
    "homepage": "index.html"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "{aa7b057d-721f-4ed8-ae73-563f767006ee}",
      "update_url": "https://raw.githubusercontent.com/mraif13/Saturn-startpage/master/updates.json"
    }
  },

  "permissions": ["storage"],
  "content_security_policy": "script-src; object-src 'self';"
}
